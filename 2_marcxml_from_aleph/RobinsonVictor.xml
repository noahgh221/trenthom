<marc:controlfield tag="003">OCoLC</marc:controlfield><marc:controlfield tag="005">20121128150021.0</marc:controlfield><marc:controlfield tag="008">950503i19101947xxu                 eng d</marc:controlfield><marc:datafield tag="035" ind1=" " ind2=" "><marc:subfield code="a">32410730</marc:subfield></marc:datafield><marc:datafield tag="040" ind1=" " ind2=" "><marc:subfield code="a">NDK</marc:subfield><marc:subfield code="c">NDK</marc:subfield></marc:datafield><marc:datafield tag="049" ind1=" " ind2=" "><marc:subfield code="a">NDNX</marc:subfield></marc:datafield><marc:datafield tag="096" ind1=" " ind2=" "><marc:subfield code="a">x</marc:subfield></marc:datafield><marc:datafield tag="100" ind1="1" ind2=" "><marc:subfield code="a">Robinson, Victor,</marc:subfield><marc:subfield code="d">1886-1947.</marc:subfield></marc:datafield><marc:datafield tag="245" ind1="1" ind2="0"><marc:subfield code="a">Papers,</marc:subfield><marc:subfield code="f">1910-1947 and undated.</marc:subfield></marc:datafield><marc:datafield tag="300" ind1=" " ind2=" "><marc:subfield code="a">15</marc:subfield><marc:subfield code="f">items.</marc:subfield></marc:datafield><marc:datafield tag="545" ind1=" " ind2=" "><marc:subfield code="a">U.S. historian of medicine and editor.</marc:subfield></marc:datafield><marc:datafield tag="500" ind1=" " ind2=" "><marc:subfield code="a">Forms part of the Trent Manuscripts Collection.</marc:subfield></marc:datafield><marc:datafield tag="520" ind1="8" ind2=" "><marc:subfield code="a">Material includes 9 TLS and 2 ALS. Most letters are addressed to Victor Robinson, as editor of the journal &quot;Medical life.&quot; One letter is addressed to William J. Robinson, editor of &quot;The critic and guide.&quot; Correspondents include Otto Juettner, his wife Estelle, Henry E. Sigerist, Arturo Castiglioni, G. C. Grippen, J. R. de la Torre-Bueno, Sarah I. Morris and Alfred A. Knopf. Grippen requests a review of Sir Marc Armand Ruffer's book, &quot;Studies in the palaeopathology of Egypt.&quot; Torre-Bueno requests a review of E. B. Krumbhaar's English translation of Castiglioni's work, &quot;A history of medicine.&quot; Juettner submits his work, &quot;Daniel Drake and his followers&quot; for review. Material also includes a bookplate and Christmas card of Castiglioni as well as a newsprint article regarding and the program of a farewell dinner for Sigerist.</marc:subfield></marc:datafield><marc:datafield tag="600" ind1="1" ind2="2"><marc:subfield code="a">Robinson, William J. q (William Josephus),</marc:subfield><marc:subfield code="d">1867-1936.</marc:subfield></marc:datafield><marc:datafield tag="600" ind1="1" ind2="2"><marc:subfield code="a">Juettner, Otto,</marc:subfield><marc:subfield code="d">1865-1922.</marc:subfield><marc:subfield code="t">Daniel Drake and his followers.</marc:subfield></marc:datafield><marc:datafield tag="600" ind1="1" ind2="2"><marc:subfield code="a">Ruffer, Marc Armand,</marc:subfield><marc:subfield code="c">Sir,</marc:subfield><marc:subfield code="d">1859-1917.</marc:subfield><marc:subfield code="t">Studies in the palaeopathology of Egypt.</marc:subfield></marc:datafield><marc:datafield tag="600" ind1="1" ind2="2"><marc:subfield code="a">Sigerist, Henry E.</marc:subfield><marc:subfield code="q">(Henry Ernest),</marc:subfield><marc:subfield code="d">1891-1957.</marc:subfield></marc:datafield><marc:datafield tag="600" ind1="1" ind2="2"><marc:subfield code="a">Castiglioni, Arturo,</marc:subfield><marc:subfield code="d">1874-1953.</marc:subfield><marc:subfield code="t">History of medicine.</marc:subfield></marc:datafield><marc:datafield tag="600" ind1="1" ind2="2"><marc:subfield code="a">Krumbhaar, E. B.</marc:subfield><marc:subfield code="q">(Edward Bell),</marc:subfield><marc:subfield code="d">1882-1966.</marc:subfield></marc:datafield><marc:datafield tag="600" ind1="1" ind2="2"><marc:subfield code="a">Torre-Bueno, J. R. de la</marc:subfield><marc:subfield code="q">(Jose Rollin),</marc:subfield><marc:subfield code="d">1871-</marc:subfield></marc:datafield><marc:datafield tag="600" ind1="1" ind2="2"><marc:subfield code="a">Knopf, Alfred A.,</marc:subfield><marc:subfield code="d">1892-1984.</marc:subfield></marc:datafield><marc:datafield tag="650" ind1=" " ind2="2"><marc:subfield code="a">History of Medicine.</marc:subfield></marc:datafield><marc:datafield tag="710" ind1="2" ind2=" "><marc:subfield code="a">History of Medicine Collections (Duke University)</marc:subfield></marc:datafield></marc:record>
