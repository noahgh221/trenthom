rc:datafield><marc:datafield tag="600" ind1="1" ind2="0"><marc:subfield code="a">Bergh, Rudolph,</marc:subfield><marc:subfield code="d">1824-1909.</marc:subfield></marc:datafield><marc:datafield tag="650" ind1=" " ind2="2"><marc:subfield code="a">Mollusca.</marc:subfield></marc:datafield><marc:datafield tag="700" ind1="1" ind2="2"><marc:subfield code="a">Lynge, Herman,</marc:subfield><marc:subfield code="d">1862-</marc:subfield></marc:datafield><marc:datafield tag="700" ind1="1" ind2="2"><marc:subfield code="a">Didrichsen, F.</marc:subfield><marc:subfield code="q">(Ferdinand),</marc:subfield><marc:subfield code="d">1814-1887.</marc:subfield></marc:datafield><marc:datafield tag="700" ind1="1" ind2="2"><marc:subfield code="a">Leydig, Franz,</marc:subfield><marc:subfield code="d">1821-1908.</marc:subfield></marc:datafield><marc:datafield tag="700" ind1="1" ind2="2"><marc:subfield code="a">Berthelot, M.</marc:subfield><marc:subfield code="q">(Marcellin),</marc:subfield><marc:subfield code="d">1827-1907.</marc:subfield></marc:datafield><marc:datafield tag="710" ind1="2" ind2=" "><marc:subfield code="a">History of Medicine Collections (Duke University)</marc:subfield></marc:datafield></marc:record>
